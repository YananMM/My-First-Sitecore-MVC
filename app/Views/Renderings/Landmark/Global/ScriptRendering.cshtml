@using Landmark.Classes
@using Landmark.Helper
@using Sitecore.Data.Items
<!-- Social App Ids -->
    <script>
        // TODO: Change this App Id, anc change your app 'Site URL' settings
        window.fbAppId = '@ItemGuids.LandmarkConfigItem.Fields["Facebook Application App Key"].Value';
        window.wbAppKey = '@ItemGuids.LandmarkConfigItem.Fields["Weibo Application API Key"].Value';
    </script>
<!-- Social App Ids End -->

<link rel="stylesheet" type="text/css" href="/files/landmark/css/jquery.bxslider.css"></link>
<link rel="stylesheet" type="text/css" href="/files/landmark/css/bootstrap-select.css"></link>
<!-- Dynamically generated global variables for the language options for date picker by Sitecore -->
<script>
    var __gDatePickerRegion = "";	// use '' for EN otherwise use sc:language() value
</script>
<!-- static JS lib, should be compiled into 1 single JS -->
<script src="/files/landmark/js/lib/jquery-1.11.2.min.js"></script>
<script src="/files/landmark/js/lib/jquery-migrate-1.2.1.min.js"></script>
<script src="/files/landmark/js/lib/modernizr-2.6.2.min.js"></script>
<script src="/files/landmark/js/lib/respond.js"></script>
<script src="/files/landmark/js/lib/jquery.touchSwipe.min.js"></script>
<script src="/files/landmark/js/lib/TweenMax.min.js"></script>
<script src="/files/landmark/js/lib/iscroll.js"></script>
<script src="/files/landmark/js/lib/jquery.scrollmagic.min.js"></script>
<script src="/files/landmark/js/lib/jquery.scrollmagic.debug.js"></script>
<script src="/files/landmark/js/lib/jquery.mousewheel.min.js"></script>
<script src="/files/landmark/js/lib/jquery.scrollTo.min.js"></script>
<script src="/files/landmark/js/lib/jquery.bxslider.min.js"></script>
<script src="/files/landmark/js/lib/jquery.lazyloadxt.min.js"></script>
<script src="/files/landmark/js/lib/jquery.lazyloadxt.bg.min.js"></script>
<script src="/files/landmark/js/lib/bootstrap.min.js"></script>
<script src="/files/landmark/js/lib/bootstrap-select.js"></script>
<!-- Has problems on ie8 <script src="/files/landmark/js/lib/masonry.pkgd.min.js"></script> -->
@if (LandmarkHelper.GetPageBodyClass().StartsWith("t13"))
{
<script src="/files/landmark/js/lib/masonry.pkgd.min.js"></script>
}
<script src="/files/landmark/js/main.js"></script>
<script>
    $.lazyLoadXT = {
        edgeX: 100,
        edgeY: 600,
        visibleOnly: false,
        checkDuplicates: false,
        updateEvent: 'load orientationchange resize scroll shown.bs.collapse hidden.bs.collapse lazyupdate'
    };
</script>
@if (LandmarkHelper.GetPageBodyClass().StartsWith("t7") || LandmarkHelper.GetPageBodyClass().StartsWith("t22"))
{
    <script type="text/javascript" src="/files/landmark/js/lib/svgmap/js/hammer.min.js"></script>
    <script type="text/javascript" src="/files/landmark/js/lib/svgmap/js/jquery.easing.js"></script>
    <script type="text/javascript" src="/files/landmark/js/lib/svgmap/js/smoothscroll.js"></script>
    <script type="text/javascript" src="/files/landmark/js/lib/svgmap/mapplic/mapplic.js"></script>
}
