@using Landmark.Classes
@using Landmark.Helper
@using Sitecore.Data.Fields
@using Sitecore.Data.Items
@using Landmark.Models
@model RenderingModel
@{
    MagezineHelper helper = new MagezineHelper();
    var setting = helper.GetStorySetting(Sitecore.Context.Item);
}
@if (setting.StyleA)
{
    Html.Partial("T23StyleAPartial");
}
else if (setting.StyleB)
{
    Html.Partial("T23StyleBPartial");
}
else if (setting.StyleCD)
{
    Html.Partial("T23StyleCDPartial");
}

