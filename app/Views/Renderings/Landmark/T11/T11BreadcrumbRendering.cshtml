@using Sitecore.Data.Items
@model RenderingModel

@{
    var homePage = Sitecore.Context.Database.Items["/sitecore/content/Home/Landmark"];
    var currentPage = Model.PageItem;
    var currentId = currentPage.ID;
    List<Item> items = new List<Item>();
    while (currentPage.ID != homePage.ID)
    {
        items.Add(currentPage);
        currentPage = currentPage.Parent;
    }
    items.Add(homePage);
    items.Reverse();
}

