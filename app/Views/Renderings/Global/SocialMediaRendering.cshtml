@model RenderingModel
@using Landmark.Helper
@{
    var mediaItems = Model.Item.Children.ToList();
    string src = Html.Sitecore().ImageFieldSrc("Social Media Image", Model.Item);
}

<a href="">NEWSLETTER SIGNUP</a>
<div class="footer-social">
    @*<a href="#" title="facebook"><span class="icomoon-facebook"></span></a>
        <a href="#" title="instagram"><span class="icomoon-instagram"></span></a>
        <a href="#" title="sina"><span class="icomoon-sina-weibo"></span></a>*@
    @foreach (var item in mediaItems)
    {
        Html.Sitecore().Field("Social Media Link", item, new { text = "<img data-src='" + src + "'>", title = item["Social Media Title"] });
    }
</div>